{
    "config": {
        "regression-test": 1,
        "psa-compliance-test": 0,
        "wait-for-sync": 1,
        "psa-test-ram": {
            "help": "Start of the memory used by the PSA test framework, see api-tests/platform/targets/<target>/target.cfg in psa-arch-tests",
            "required": true
        }
    },
    "target_overrides": {
        "*": {
             "platform.stdio-convert-newlines": true,
             "platform.stdio-baud-rate": 115200,
             "rtos.mutex-num": 4,
             "rtos.semaphore-num": 4,
             "rtos.thread-num": 9,
             "rtos.thread-user-stack-size": 8096,
             "target.features_add": [
                 "EXPERIMENTAL_API"
             ]
        },
        "ARM_MUSCA_B1": {
            "psa-test-ram": "0x20060000"
        },
        "ARM_MUSCA_S1": {
            "psa-test-ram": "0x20060000"
        }
    },
    "macros": [
        "TFM_ENABLE_IRQ_TEST=1",
        "TFM_MULTI_CORE_TEST=1",
        "TFM_MULTI_CORE_MULTI_CLIENT_CALL=1",
        "NUM_MAILBOX_QUEUE_SLOT=4",
        "CMSDK_TIMER0_S"
    ]
}
